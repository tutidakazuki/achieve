<div class="container">
  <div class="wrapper col-md-8 col-md-offset-2 col-sm-10">

    <h3>以下の内容で送信する。</h3>
    <%= form_for(@blog) do |f| %>
      <%= f.hidden_field :title %>
      <%= f.hidden_field :content %>
      <p>タイトル:<%= @blog.title %></p>
      <p>タイトル:<%= @blog.content %></p>
      <%= f.submit "登録",class: 'btn btn-primary btn-block' %>
    <% end %>
    <%= form_for(@blog,url:new_blog_path, method: 'get')do |f| %>
      <%= f.hidden_field :title %>
      <%= f.hidden_field :content %>
      <%= f.submit "もどる", name: 'back',class: 'btn btn-primary btn-block' %>
    <% end %>
  </div>
</div>